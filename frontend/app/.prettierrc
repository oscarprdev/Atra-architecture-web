{
    "printWidth": 90,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "astroAllowShorthand": false,
    "singleAttributePerLine": true,
    "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss", "prettier-plugin-vue"]
}
